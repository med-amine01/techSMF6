{% extends 'sb_admin_template/index.html.twig' %}

{% block PageTitle %}Societe{% endblock %}

{% block body %}

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ societe.id }}</td>
            </tr>
            <tr>
                <th>NameSoc</th>
                <td>{{ societe.nameSoc }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ societe.createdAt ? societe.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ societe.updatedAt ? societe.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>


    <a class="btn btn-warning" href="{{ path('app_societe_edit', {'id': societe.id}) }}">edit</a>

    {{ include('societe/_delete_form.html.twig') }}

    <a href="{{ path('app_societe_index') }}">back to list</a>
{% endblock %}
